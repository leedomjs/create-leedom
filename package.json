{
  "name": "create-leedom",
  "type": "module",
  "version": "0.6.0",
  "packageManager": "pnpm@8.6.11",
  "description": "An easy way to create a new project",
  "keywords": [
    "cli",
    "leedomjs",
    "create-leedom"
  ],
  "author": "Leedom",
  "license": "MIT",
  "homepage": "https://github.com/leedomjs/create-leedom#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leedomjs/create-leedom.git"
  },
  "bugs": {
    "url": "https://github.com/leedomjs/create-leedom/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "create-leedom": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "unbuild && bumpp && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "degit": "^2.8.4",
    "glob": "^10.3.12",
    "gradient-string": "^2.0.2",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@leedomjs/eslint-config-ts": "^0.9.0",
    "@types/node": "^20.12.2",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
}
